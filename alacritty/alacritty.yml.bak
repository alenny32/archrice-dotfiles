window:
  padding:
    x: 5
    y: 5
  class:
    instance: Alacritty
    general: alacritty
  opacity: 0.8

scrolling:
  history: 10000
  multiplier: 3


font:
  normal:
    family: FiraCode Nerd Font
    style: Medium
  bold:
    family: FiraCode Nerd Font
    style: Bold
  italic:
    family: JetBrainsMono Nerd Font
    style: MediumItalic
  bold_italic:
    family: JetBrainsMono Nerd Font
    style: BoldItalic
  size: 10
draw_bold_text_with_bright_colors: true

  ### Colors ###

  # ## Main colors ##
# colors:
  # primary:
